FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      wallShearStress;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    septal
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    lateral
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    nostril
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    sides
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    nasopharynx
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    turbinates
    {
        type            calculated;
        value           uniform (0 0 0);
    }
}


